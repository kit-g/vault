{
    "components": {
        "schemas": {
            "AttachmentOut": {
                "properties": {
                    "filename": {
                        "example": "document.pdf",
                        "type": "string"
                    },
                    "id": {
                        "example": "123e4567-e89b-12d3-a456-426614174000",
                        "type": "string"
                    },
                    "mime_type": {
                        "example": "application/pdf",
                        "type": "string"
                    },
                    "size": {
                        "example": 123456,
                        "type": "integer"
                    }
                },
                "type": "object"
            },
            "ErrorResponse": {
                "properties": {
                    "code": {
                        "example": "InternalError",
                        "type": "string"
                    },
                    "error": {
                        "example": "An unexpected error occurred",
                        "type": "string"
                    }
                },
                "type": "object"
            },
            "Login": {
                "properties": {
                    "email": {
                        "example": "jane@mail.com",
                        "type": "string"
                    },
                    "password": {
                        "type": "string"
                    }
                },
                "required": [
                    "email",
                    "password"
                ],
                "type": "object"
            },
            "LoginOut": {
                "properties": {
                    "session": {
                        "$ref": "#/components/schemas/Session"
                    },
                    "user": {
                        "$ref": "#/components/schemas/UserOut"
                    }
                },
                "type": "object"
            },
            "NoteIn": {
                "properties": {
                    "content": {
                        "example": "Notes from the meeting with the client.",
                        "type": "string"
                    },
                    "title": {
                        "example": "Meeting Notes",
                        "type": "string"
                    }
                },
                "required": [
                    "content",
                    "title"
                ],
                "type": "object"
            },
            "NoteOut": {
                "properties": {
                    "archived": {
                        "type": "boolean"
                    },
                    "attachments": {
                        "items": {
                            "$ref": "#/components/schemas/AttachmentOut"
                        },
                        "type": "array"
                    },
                    "content": {
                        "example": "Notes from the meeting with the client.",
                        "type": "string"
                    },
                    "created_at": {
                        "type": "string"
                    },
                    "encrypted": {
                        "type": "boolean"
                    },
                    "id": {
                        "example": "123e4567-e89b-12d3-a456-426614174000",
                        "type": "string"
                    },
                    "title": {
                        "example": "Meeting Notes",
                        "type": "string"
                    },
                    "updated_at": {
                        "type": "string"
                    }
                },
                "type": "object"
            },
            "PresignDownloadResponse": {
                "properties": {
                    "url": {
                        "type": "string"
                    }
                },
                "type": "object"
            },
            "PresignUploadRequest": {
                "properties": {
                    "content_type": {
                        "example": "text/plain",
                        "type": "string"
                    },
                    "filename": {
                        "example": "example.txt",
                        "type": "string"
                    }
                },
                "required": [
                    "content_type",
                    "filename"
                ],
                "type": "object"
            },
            "PresignUploadResponse": {
                "properties": {
                    "key": {
                        "example": "attachments/123e4567-e89b-12d3-a456-426614174000/example.txt",
                        "type": "string"
                    },
                    "url": {
                        "example": "https://s3.com/upload?key=example.txt",
                        "type": "string"
                    }
                },
                "type": "object"
            },
            "Session": {
                "properties": {
                    "refresh": {
                        "type": "string"
                    },
                    "token": {
                        "type": "string"
                    }
                },
                "type": "object"
            },
            "ShareToUserRequest": {
                "properties": {
                    "permission": {
                        "description": "\"read\" or \"write\"",
                        "example": "read",
                        "type": "string"
                    },
                    "shared_with": {
                        "example": "123e4567-e89b-12d3-a456-426614174000",
                        "type": "string"
                    }
                },
                "required": [
                    "permission",
                    "shared_with"
                ],
                "type": "object"
            },
            "UserIn": {
                "properties": {
                    "email": {
                        "example": "jane@mail.com",
                        "type": "string"
                    },
                    "password": {
                        "example": "password123",
                        "minLength": 6,
                        "type": "string"
                    },
                    "username": {
                        "example": "jane_doe",
                        "minLength": 1,
                        "type": "string"
                    }
                },
                "required": [
                    "email",
                    "password",
                    "username"
                ],
                "type": "object"
            },
            "UserOut": {
                "properties": {
                    "email": {
                        "example": "jane@mail.com",
                        "type": "string"
                    },
                    "id": {
                        "example": "123e4567-e89b-12d3-a456-426614174000",
                        "type": "string"
                    },
                    "username": {
                        "example": "jane_doe",
                        "type": "string"
                    }
